{
  "name": "goldfish",
  "version": "4.0.0",
  "description": "Transparent developer memory system for AI agents - iteration #4, back to basics",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "dev": "bun run src/server.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "build": "bun build src/server.ts --outdir dist --target node",
    "typecheck": "tsc --noEmit",
    "setup": "bun run setup.ts",
    "setup:force": "bun run setup.ts --force",
    "setup:reset": "bun run setup.ts reset",
    "setup:status": "bun run setup.ts status",
    "migrate": "bun run scripts/migrate-embeddings.ts",
    "migrate:all": "bun run scripts/migrate-embeddings.ts --all"
  },
  "keywords": [
    "mcp",
    "memory",
    "ai-agent",
    "developer-tools",
    "claude-code"
  ],
  "author": "murphy",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@napi-rs/blake-hash": "^1.3.5",
    "@xenova/transformers": "^2.17.0",
    "fuse.js": "^7.0.0",
    "hnswlib-node": "^1.4.2",
    "onnxruntime-node": "^1.16.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
